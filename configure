					cf_config="$cf_libpath"
					break
				fi
				;;
			esac
		done
		if test -z "$cf_config" ; then
			{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: Could not find imake config-directory" >&5
printf "%s\n" "$as_me: WARNING: Could not find imake config-directory" >&2;}
		else
			cf_imake_opts="$cf_imake_opts -I$cf_config"
