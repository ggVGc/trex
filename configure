		else
			{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: not a directory" >&5
printf "%s\n" "$as_me: WARNING: not a directory" >&2;}
		fi
	elif test "$prefix" != NONE ; then
		TERMINFO_DIR='${prefix}/lib/terminfo'
