		TERMINFO_DIR=/usr/lib/terminfo
	else
		TERMINFO_DIR=
	fi
	SET_TERMINFO=
	if test -n "$TERMINFO_DIR" ; then
		TERMINFO_DIR='${DESTDIR}'$TERMINFO_DIR
		SET_TERMINFO='TERMINFO=${TERMINFO_DIR}'
	fi
